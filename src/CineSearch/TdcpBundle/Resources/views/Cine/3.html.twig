<script type="text/javascript">

  $('#NLsub').hide();
  $(document).ready(function(){

    $('#NLsub').removeClass('perte-desktop');
    $('#NLsub').removeClass('perte-mobil');

    $(window).one("scroll",function () {

      $(location).attr('href');
      var pathname = window.location.href;
      var shownl = function(){
       $('#NLsub').toggleClass('NLdown')
     };

     if (pathname.indexOf("NL") >= 0){}
      else{
        $('#NLsub').delay( 1500 ).show(0);
        setTimeout(shownl, 2000);
      };

    });
    $("#NLsuscribe").submit(function(e) {
      e.preventDefault();
      $('#NLsubmit').hide();
    });

    $('.closeNL').click(function(e){
      $('.Newsletterdude').hide();
    })})

  var today =new Date();
  var dd = today.getDate();
  var mm = today.getMonth()+1;
  var top_level_div = document.getElementById('resultats-vil');
  var count = top_level_div.getElementsByTagName('li').length;
  var yyyy = today.getFullYear();
  if(dd<10){
    dd='0'+dd
  }
  if(mm<10){
    mm='0'+mm
  }
  var today = yyyy+'-'+mm+'-'+dd;
  for (i = 1; i <= count ; i++) {
    var d = document.getElementById(today+i);
    var donglet = document.getElementById(i+today);
    d.className = d.className + " active ";
    donglet.className = donglet.className + " active in";
  }
</script>

